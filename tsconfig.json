{
  "compilerOptions": {
    "target": "esnext",
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true,
    "outDir": "./dist",
    "baseUrl": "./src",
    "paths": {
      "#/*": ["*"],
      "#rcon/*": ["rcon/*"]
    }
  },
  "exclude": ["node_modules"]
}
